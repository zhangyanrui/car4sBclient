<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <include layout="@layout/common_actionbar"></include>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <cn.car4s.app.ui.widget.SettingLayoutSmall
                android:layout_width="match_parent"
                android:id="@+id/setting_keyongjifen"
                android:layout_height="51dp">

        </cn.car4s.app.ui.widget.SettingLayoutSmall>

        <cn.car4s.app.ui.widget.SettingLayoutSmall
                android:layout_width="match_parent"
                android:id="@+id/setting_dongjiejifen"
                android:layout_height="51dp">

        </cn.car4s.app.ui.widget.SettingLayoutSmall>

        <cn.car4s.app.ui.widget.SettingLayoutSmall
                android:layout_width="match_parent"
                android:id="@+id/setting_feedback"
                android:layout_height="51dp">

        </cn.car4s.app.ui.widget.SettingLayoutSmall>
        <cn.car4s.app.ui.widget.SettingLayoutSmall
                android:layout_width="match_parent"
                android:id="@+id/setting_aboutus"
                android:layout_height="51dp">
        </cn.car4s.app.ui.widget.SettingLayoutSmall>

    </LinearLayout>

</LinearLayout>